#include "/usr/share/nord"

*font: xft:Sauce\ Code\ Pro\ Nerd\ Font:pixelsize=15
rofi.font: xft:Sauce\ Code\ Pro\ Nerd\ Font\ 15

Xft.dpi: 96
Xft.antialias: 1
Xft.autohint: 1
Xft.hinting: 1
Xft.hintstyle: hintslight
Xft.lcdfilter: lcdlight
Xft.rgba: rgb

URxvt.scrollBar: false
URxvt.internalBorder: 3
URxvt.perl-ext-common: default,matcher
URxvt.matcher.button: 1
URxvt.matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]
URxvt.url-launcher: xdg-open
URxvt.letterSpace: 1
URxvt.transparant: true
URxvt.depth: 32
URxvt.shading: 10
urxvt*blurRadius: 5
URxvt*termName: rxvt-unicode
URxvt.keysym.Home: \033[1~
URxvt.keysym.End: \033[4~
URxvt.keysym.KP_Home: \033[1~
URxvt.keysym.KP_End:  \033[4~

URxvt*foreground:   #D8DEE9
URxvt*background:   [70]#2E3440
URxvt*cursorColor:  #E5E9F0
URxvt*borderColor:  [70]#2E3440

/* *foreground:        #D8DEE9 */
/* *background:        #2E3440 */
/* *.foreground:       #D8DEE9 */
/* *.background:       #2E3440 */

emacs*foreground:   #D8DEE9
emacs*background:   #2E3440

XTerm*foreground:   #D8DEE9
XTerm*background:   #2E3440
XTerm*cursorColor:  #E5E9F0

UXTerm*foreground:  #D8DEE9
UXTerm*background:  #2E3440
UXTerm*cursorColor: #E5E9F0

! Xclock colors
XClock*foreground: #D8DEE9
XClock*background: #2E3440
XClock*majorColor:  rgba:b4/8e/ad/ff
XClock*minorColor:  rgba:b4/8e/ad/ff
XClock*hourColor:   rgba:b4/8e/ad/ff
XClock*minuteColor: rgba:b4/8e/ad/ff
XClock*secondColor: rgba:b4/8e/ad/ff


